{
    "name": "@plurid/pluridsheet-react",
    "version": "0.0.0-0",
    "description": "Plurid-Based Spreadsheet Space for React",
    "keywords": [
        "plurid",
        "spreadsheet",
        "react"
    ],
    "author": "ly3xqhl8g9 <ly3xqhl8g9@plurid.com> (https://plurid.com)",
    "license": "SEE LICENSE IN LICENSE",
    "repository": {
        "type": "git",
        "url": "https://github.com/plurid/pluridsheet"
    },
    "bugs": {
        "email": "source@plurid.com",
        "url": "https://github.com/plurid/pluridsheet/issues"
    },
    "homepage": "https://github.com/plurid/pluridsheet",
    "publishConfig": {
        "registry": "https://registry.npmjs.org/",
        "access": "public"
    },
    "files": [
        "distribution/"
    ],
    "main": "distribution/index.js",
    "module": "distribution/index.es.js",
    "types": "distribution/index.d.ts",
    "engines": {
        "node": ">=12",
        "npm": ">=6"
    },
    "scripts": {
        "storybook": "start-storybook -p 6006",
        "build.storybook": "build-storybook",
        "test": "jest -c ./configurations/jest.config.js",
        "lint": "eslint -c ./configurations/.eslintrc.js ./source/index.ts",
        "clean": "rm -rf ./distribution",
        "start": "yarn clean && rollup -c ./scripts/rollup.config.js -w",
        "build.clean": "rm -rf `find ./distribution/ -type d -name __tests__` && rm -rf `find ./distribution/ -type d -name __stories__` && rm -rf ./distribution/utilities/storybook",
        "build.development": "yarn clean && rollup -c ./scripts/rollup.config.js",
        "build.production": "rollup -c ./scripts/rollup.config.js --environment BUILD:production",
        "build": "yarn clean && yarn lint && yarn test && yarn build.production && yarn build.clean",
        "prepublishOnly": "yarn build"
    },
    "peerDependencies": {
        "@plurid/elementql": "*",
        "@plurid/elementql-client-react": "*",
        "@plurid/plurid-data": "*",
        "@plurid/plurid-engine": "*",
        "@plurid/plurid-functions": "*",
        "@plurid/plurid-functions-react": "*",
        "@plurid/plurid-icons-react": "*",
        "@plurid/plurid-pubsub": "*",
        "@plurid/plurid-react": "*",
        "@plurid/plurid-themes": "*",
        "@plurid/plurid-ui-components-react": "*",
        "@plurid/plurid-ui-state-react": "*",
        "react": ">=17",
        "react-dom": ">=17",
        "react-redux": ">=7.2",
        "redux": ">=4",
        "redux-thunk": ">=2.3",
        "styled-components": ">=5"
    },
    "devDependencies": {
        "@plurid/elementql": "^0.0.0-1",
        "@plurid/elementql-client-react": "^0.0.0-1",
        "@plurid/plurid-data": "^0.0.0-20",
        "@plurid/plurid-engine": "^0.0.0-18",
        "@plurid/plurid-functions": "^0.0.0-30",
        "@plurid/plurid-functions-react": "^0.0.0-5",
        "@plurid/plurid-icons-react": "^0.0.0-5",
        "@plurid/plurid-pubsub": "^0.0.0-9",
        "@plurid/plurid-react": "^0.0.0-32",
        "@plurid/plurid-themes": "^0.0.0-2",
        "@plurid/plurid-ui-components-react": "^0.0.0-19",
        "@plurid/plurid-ui-state-react": "^0.0.0-8",
        "@svgr/rollup": "^6.3.1",
        "@types/enzyme": "^3.10.12",
        "@types/enzyme-adapter-react-16": "^1.0.6",
        "@types/jest": "^29.1.1",
        "@types/react": "^18.0.21",
        "@types/react-dom": "^18.0.6",
        "@types/react-redux": "^7.1.24",
        "@types/styled-components": "^5.1.26",
        "@typescript-eslint/eslint-plugin": "^5.38.1",
        "@typescript-eslint/parser": "^5.38.1",
        "@zerollup/ts-transform-paths": "^1.7.18",
        "babel-loader": "^8.2.5",
        "cross-fetch": "^3.1.5",
        "eslint": "^8.24.0",
        "hammerjs": "^2.0.8",
        "jest": "^29.1.1",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-redux": "^8.0.4",
        "react-test-renderer": "^18.2.0",
        "redux": "^4.2.0",
        "redux-thunk": "^2.4.1",
        "rollup": "^2.79.1",
        "rollup-plugin-terser": "^7.0.2",
        "rollup-plugin-typescript2": "^0.34.0",
        "styled-components": "^5.3.6",
        "ts-jest": "^29.0.3",
        "ttypescript": "^1.5.13",
        "typescript": "^4.8.4"
    }
}
