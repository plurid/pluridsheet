        "react": ">=17",
        "react-dom": ">=17",
        "react-redux": ">=7.2",
        "redux": ">=4",
        "redux-thunk": ">=2.3",
        "styled-components": ">=5"
    },
    "devDependencies": {
        "@babel/core": "^7.16.0",
        "@plurid/elementql": "^0.0.0-1",
