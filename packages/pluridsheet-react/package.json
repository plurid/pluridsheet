        "@plurid/plurid-engine": "^0.0.0-7",
        "@plurid/plurid-functions": "^0.0.0-16",
        "@plurid/plurid-functions-react": "^0.0.0-4",
        "@plurid/plurid-icons-react": "^0.0.0-1",
